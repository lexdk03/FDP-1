#!/bin/bash
clear
int=1
teste=0
maiorOrd=0
tmp=0
while (( $int != 0 )); do
	read -p "Digite um numero" num[$teste]
	int=${num[$teste]}
	let teste=$teste+1
	t=$teste
	te=$t
	tee=$te
done
	let te=$te-2
	let tee=$tee-1
echo ${num[$teste]}
echo ${num[$te]}
