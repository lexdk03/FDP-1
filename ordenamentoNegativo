for (( u=0; u < $teste; u++ )); do
	for (( y=0; y < $teste; y++ )); do
		if (( $maiorOrd <= ${num[y]} )); then
			maiorOrd=${num[$y]}
			x=$y
			maior[$t]=$maiorOrd
		fi
	done
	num[$x]=0
	let t=$t-1
	maiorOrd=0
done
echo ${maior[*]}
